<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Edit | The neighborhood</title>
    <link rel="stylesheet" href="/css/base.css">
    <link rel="stylesheet" href="/css/controlpanel.css">

</head>

<body>
    <%- include("partials/header") %>
    <main>
        <div class="fileslist">
            <% files.forEach(File => { %>
            <a class="file"><%= File %></a>&nbsp;&nbsp;&nbsp;&nbsp;<a class="deletefile" href="/House/deletefile/<%= File %>">X</a><br>
            <% }) %>
        </div>
        <form method="post" action="/House/addfiles" enctype="multipart/form-data" target="backgroundworkeriframe">
            <input type="file" name="file" id="file" multiple>
            <input type="submit" value="Upload Furnature">
        </form>
    </main>
</body>
</html>